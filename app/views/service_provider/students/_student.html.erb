<%= content_tag_for(:tr,student,:class=>cycle('even', 'odd')) do -%>
<td><%= student.following_assessors.size %></td>
<td><%= student.name %></td>
<td><%= highlight(student.email,params[:search]) %></td>
<td><%= student.created_at %></td>
<td>
 <div id="approve_<%=student.id%>">
   <%=link_to student.approved? ? image_tag('tick.gif') : image_tag('cros.gif') ,approve_service_provider_student_path(student),:remote=>true %>
 </div>
</td>
<td>

</td>
<% end %>




