<% form_for :upload, :url=>{:action=>"csv_import"},:html => { :multipart => true } do |f| -%>
<table>
<tr>
    <td>
        <label for="vle_user">
            Select a CSV File :
        </label>
    </td>
    <td>
        <%= f.file_field :file -%>
    </td>
</tr>
<tr>
    <td colspan='2'>
        <%= submit_tag 'Submit' -%>
    </td>
</tr>
</table>
<% end -%>