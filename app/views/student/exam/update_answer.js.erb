<% if @active_question.correct_answer.nil? %>
$("#active_question_<%= params[:id] %>").attr('class', 'current viewed')
$("#active_question_<%= params[:id] %>").attr('title', 'Viewed')
<% else %>
$("#active_question_<%= params[:id] %>").attr('class', 'current completed')
$("#active_question_<%= params[:id] %>").attr('title', 'Completed')
<% end %>



